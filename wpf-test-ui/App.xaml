<Application x:Class="wpf_test_ui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:wpf_test_ui"
             xmlns:vm="clr-namespace:wpf_test_ui.ViewModels"
             xmlns:uc="clr-namespace:wpf_test_ui.UserControls"
             StartupUri="Views\Shell.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/DarkStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <DataTemplate DataType="{x:Type vm:PumpControlViewModel}">
                <uc:PumpControlUserControl />
            </DataTemplate>

        </ResourceDictionary>


        
    </Application.Resources>
    
    
</Application>
